{
    "version": "0.1.0",
    "command": "C:\\Python27", 
    "args": ["$(file)"],
    "isShellCommand": true,
    "showOutput": "always",
    "problemMatcher": {
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern": {
            "regexp": "^(.*)+s$",
            "message": 1
        }
    }
}